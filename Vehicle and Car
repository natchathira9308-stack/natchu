import java.io.*;
import java.util.*;

class Vehicle{
    void Brand(String str){
        System.out.println("Brand:"+str);   
    }
}
    class Speed extends Vehicle{
        void Car(int n){
            System.out.println("Speed:"+n);
        }
    }
public class main{
    
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        String str=s.nextLine();
        int n=s.nextInt();
        Speed obj=new Speed();
        obj.Brand(str);
        obj.Car(n);
        
    }
    
}
        
